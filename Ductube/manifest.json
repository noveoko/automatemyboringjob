{
    "name": "Youtube:Filter",
    "version": "1.0.0",
    "description": "Ignore YT content that's annoying",
    "manifest_version": 2,
    "author": "Marcin Kraszewski",
    "action":{
        "default_icon": "icon.png",
        "default_popup": "index.html",
        "default_title": "YT Filter"
    },
    "browser_action":{
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
        "matches": ["https://youtube.com/*","https://www.youtube.com/"],
        "js": ["script.js"]
        }
    ]
}