{
    "name": "GoogleSearch:Filter",
    "version": "1.0.0",
    "description": "Ignore Google content that's annoying",
    "manifest_version": 2,
    "author": "Marcin Kraszewski",
    "action":{
        "default_icon": "icon.png",
        "default_popup": "index.html",
        "default_title": "Google Filter"
    },
    "browser_action":{
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
        "matches": ["https://www.google.com/search*"],
        "js": ["script.js"]
        }
    ]
}